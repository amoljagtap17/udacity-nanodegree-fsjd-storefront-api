{
  "spec_dir": "spec",
  "spec_files": ["../**/api/*[s]pec.ts", "../**/models/*[s]pec.ts"],
  "helpers": ["helpers/**/*.js"],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": false
}
